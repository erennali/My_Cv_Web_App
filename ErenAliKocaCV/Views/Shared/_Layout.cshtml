<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no"/>
    <title>@ViewData["Title"] - ErenAliKocaCV</title>
    
    <link href="https://fonts.googleapis.com/css?family=Poppins:100,200,300,400,500,600,700,800,900" rel="stylesheet">

    <link rel="stylesheet" href="~/clark-master/css/open-iconic-bootstrap.min.css">
    <link rel="stylesheet" href="~/clark-master/css/animate.css">
    
    <link rel="stylesheet" href="~/clark-master/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/clark-master/css/owl.theme.default.min.css">
    <link rel="stylesheet" href="~/clark-master/css/magnific-popup.css">

    <link rel="stylesheet" href="~/clark-master/css/aos.css">

    <link rel="stylesheet" href="~/clark-master/css/ionicons.min.css">
    
    <link rel="stylesheet" href="~/clark-master/css/flaticon.css">
    <link rel="stylesheet" href="~/clark-master/css/icomoon.css">
    <link rel="stylesheet" href="~/clark-master/css/style.css">
    
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/ErenAliKocaCV.styles.css" asp-append-version="true"/>
</head>
<body data-spy="scroll" data-target=".site-navbar-target" data-offset="300">
    
    <nav class="navbar navbar-expand-lg navbar-dark ftco_navbar ftco-navbar-light site-navbar-target" id="ftco-navbar">
        <div class="container">
            <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">ErenAliKocaCV</a>
            <button class="navbar-toggler js-fh5co-nav-toggle fh5co-nav-toggle" type="button" data-toggle="collapse" data-target="#ftco-nav" aria-controls="ftco-nav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="oi oi-menu"></span> Menu
            </button>

            <div class="collapse navbar-collapse" id="ftco-nav">
                <ul class="navbar-nav nav ml-auto">
                    <li class="nav-item"><a href="#home-section" class="nav-link"><span>Home</span></a></li>
                    <li class="nav-item"><a href="#about-section" class="nav-link"><span>About</span></a></li>
                    <li class="nav-item"><a href="#resume-section" class="nav-link"><span>Resume</span></a></li>
                    <li class="nav-item"><a href="#services-section" class="nav-link"><span>Services</span></a></li>
                    <li class="nav-item"><a href="#skills-section" class="nav-link"><span>Skills</span></a></li>
                    <li class="nav-item"><a href="#projects-section" class="nav-link"><span>Projects</span></a></li>
                    <li class="nav-item"><a href="#contact-section" class="nav-link"><span>Contact</span></a></li>
                </ul>
            </div>
        </div>
    </nav>

    <main>
        @RenderBody()
    </main>

    <footer class="ftco-footer ftco-section">
        <div class="container">
            <div class="row">
                <div class="col-md-12 text-center">
                    <p>
                        &copy; @DateTime.Now.Year - ErenAliKocaCV
                    </p>
                </div>
            </div>
        </div>
    </footer>
    
    <!-- loader -->
    <div id="ftco-loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee"/><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#F96D00"/></svg></div>

    <script src="~/clark-master/js/jquery.min.js"></script>
    <script src="~/clark-master/js/jquery-migrate-3.0.1.min.js"></script>
    <script src="~/clark-master/js/popper.min.js"></script>
    <script src="~/clark-master/js/bootstrap.min.js"></script>
    <script src="~/clark-master/js/jquery.easing.1.3.js"></script>
    <script src="~/clark-master/js/jquery.waypoints.min.js"></script>
    <script src="~/clark-master/js/jquery.stellar.min.js"></script>
    <script src="~/clark-master/js/owl.carousel.min.js"></script>
    <script src="~/clark-master/js/jquery.magnific-popup.min.js"></script>
    <script src="~/clark-master/js/aos.js"></script>
    <script src="~/clark-master/js/jquery.animateNumber.min.js"></script>
    <script src="~/clark-master/js/scrollax.min.js"></script>
    <script src="~/clark-master/js/main.js"></script>
    
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>